    "#fibonacci series\n",
    "a=0\n",
    "b=1\n",
    "input_val=int(input(\">\"))\n",
    "for x in range(input_val):\n",
    "    if x == 0 :\n",
    "        print(a,end=\" \")\n",
    "    if x == 1:\n",
    "        print(b,end=\" \")\n",
    "    if x > 1:\n",
    "        c=a+b\n",
    "        print(c,end=\" \")\n",
    "        a=b\n",
    "        b=c  "
